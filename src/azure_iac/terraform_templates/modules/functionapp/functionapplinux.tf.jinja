resource "azurerm_linux_function_app" "{{engine.module_name}}" {
    name                = "{{engine.module_params_name}}"
    resource_group_name = azurerm_resource_group.rg.name
    location            = azurerm_resource_group.rg.location

    storage_account_name       = azurerm_storage_account.strg.name
    storage_account_access_key = azurerm_storage_account.strg.primary_access_key
    service_plan_id            = azurerm_service_plan.appplanlinux.id

    site_config {}

    identity {
        type = "SystemAssigned"
    }
}