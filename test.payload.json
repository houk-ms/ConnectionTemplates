{
	"resources": [
	  {
		"type": "azureappservice",
		"instances": [
		  {
			"name": "app1"
		  },
		  {
			"name": "app2"
		  }
		]
	  },
	  {
		"type": "azurecontainerapp",
		"instances": [
		  {
			"name": "aca1"
		  }
		]
	  },
	  {
		"type": "azuremysqldatabase",
		"instances": [
		  {
			"name": "mysql1"
		  },
		  {
			"name": "mysql2"
		  }
		]
	  },
	  {
		"type": "azurekeyvault",
		"instances": [
		  {
			"name": "kv1"
		  }
		]
	  },
	  {
		"type": "azuresqldatabase",
		"instances": [
		  {
			"name": "sql1"
		  },
		  {
			"name": "sql2"
		  }
		]
	  },
	  {
		"type": "azuredatabaseforpostgresql",
		"instances": [
			{
				"name": "pg1"
			},
			{
				"name": "pg2"
			}
		]
	  },
	  {
		"type": "azurecosmosdb",
		"instances": [
		  {
			"name": "cosmos1"
		  }
		]
	  }
	],
	"bindings": [
		{
			"source": "${azureappservice.app1}",
			"target": "${azuremysqldatabase.mysql1}",
			"customkeys": {
				"AZURE_MYSQL_CONNECTIONSTRING": "rawmysql1"
			},
			"store": "${azurekeyvault.kv1}"
		},
		{
			"source": "${azureappservice.app2}",
			"target": "${azuremysqldatabase.mysql2}",
			"connection": "secret",
			"store": "${azurekeyvault.kv1}",
			"customkeys": {
				"AZURE_MYSQL_HOST": "testhost",
				"AZURE_MYSQL_USER": "testuser",
				"AZURE_MYSQL_PASSWORD": "testpassword",
				"AZURE_MYSQL_DATABASE": "testdb",
				"AZURE_MYSQL_PORT": "testport"
			}
		},
		{
			"source": "${azureappservice.app1}",
			"target": "${azuresqldatabase.sql1}",
			"connection": "secret"
		},
		{
			"source": "${azureappservice.app2}",
			"target": "${azuresqldatabase.sql2}",
			"connection": "secret",
			"customkeys": {
				"AZURE_SQL_HOST": "testhost",
				"AZURE_SQL_USERNAME": "testuser",
				"AZURE_SQL_PASSWORD": "testpassword",
				"AZURE_SQL_DATABASE": "testdb",
				"AZURE_SQL_PORT": "0000"
			}
		},
		{
			"source": "${azureappservice.app1}",
			"target": "${azuredatabaseforpostgresql.pg1}",
			"connection": "secret",
			"store": "${azurekeyvault.kv1}",
			"customkeys": {
				"AZURE_POSTGRESQL_CONNECTIONSTRING": "pgsqlconnstr"
			}
		},
		{
			"source": "${azureappservice.app2}",
			"target": "${azuredatabaseforpostgresql.pg2}",
			"connection": "secret"
		},
		{
			"source": "${azureappservice.app1}",
			"target": "${azurecosmosdb.cosmos1}",
			"connection": "secret"
		},
		{
			"source": "${azurecontainerapp.aca1}",
			"target": "${azuremysqldatabase.mysql1}",
			"connection": "secret",
			"customkeys": {
				"AZURE_MYSQL_HOST": "pyhost"
			}
		}
		
	],
	"services": [
		{
		  "host": "${azureappservice.app1}",
		  "language": "dotnet",
		  "project": "."
		},
		{
			"host": "${azureappservice.app2}",
			"language": "js",
			"project": "."
		},
		{
			"host": "${azurecontainerapp.aca1}",
			"language": "python",
			"project": "."
		}
	  ]
}
  